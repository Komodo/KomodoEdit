    "    // other items are constants in effect", 
    "    ko.extensions.rails.CmdSequenceRunner.prototype.terminationCallback = function() {", 
    "        try {", 
    "        this.clearOutputWindow = false;", 
    "        this.cmds.shift();", 
    "        if (this.cmds.length > 0) {", 
    "            this.options.clearOutputWindow = false;", 
    "            this.runner();", 
    "        } else if (this.end_note) {", 
    "            ko.extensions.rails.append_terminal_output(this.end_note);", 
    "        }", 
    "        }catch(ex)  { alert(ex + \"\\n\"); }", 

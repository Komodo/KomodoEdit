    "                'create' : \"-u %(username) --password=%(password) %?(socket)  create %(database)\",", 
    "                'delete' : \"-u %(username) --password=%(password) %?(socket) --force drop %(database)\"", 
    "            },", 
    "            __END__ : null", 

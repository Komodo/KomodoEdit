    "    ko.extensions.rails.CmdSequenceRunner.prototype.runner = function() {", 
    "        var self = this;", 
    "        var tcb = function() {", 
    "            self.terminationCallback();", 
    "        }", 
    "        ko.extensions.rails.runCommand(this.editor, this.cmds[0], this.options, tcb);", 
    "    }", 

                                <radio id="preferProp"
                                       oncommand="preferFixed(false)"
                                       accesskey="&preferProp.accesskey;"
                                       label="&preferProp.label;"
                                       />
                            </hbox>
                            <hbox flex="1" align="center">
                                <menulist flex="1"
                                          id="proportional"
                                          style="width: 90px"
                                          oncommand="onClickProportionalFont()">
                                    <menupopup/>
                                </menulist>
                            </hbox>
                            <separator class="thin"/>
                            <hbox align="center">
                                <menulist id="propSize" oncommand="onClickProportionalSize()">
                                    <menupopup>
                                        <menuitem id="propSize_8" value="8" label="8"/>
                                        <menuitem id="propSize_9" value="9" label="9"/>
                                        <menuitem id="propSize_10" value="10" label="10"/>
                                        <menuitem id="propSize_11" value="11" label="11"/>
                                        <menuitem id="propSize_12" value="12" label="12"/>
                                        <menuitem id="propSize_13" value="13" label="13"/>
                                        <menuitem id="propSize_14" value="14" label="14"/>
                                        <menuitem id="propSize_15" value="15" label="15"/>
                                        <menuitem id="propSize_16" value="16" label="16"/>

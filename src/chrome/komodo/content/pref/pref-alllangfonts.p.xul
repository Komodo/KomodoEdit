                                                 onchange="setColour(this)"/>
                                    <button style="margin-bottom: 6px"
                                            class="arrow-right-6x3-icon button-toolbar-a"
                                            oncommand="customColor('specificColorPickerBack')"/>
                                </hbox>
                                <vbox style="margin-top: 4px;">
                                    <checkbox id="useGlobalSubLanguageBackground"
                                              oncommand="checkSubLanguageBackground(this);"
                                              label="&filePrefUseSubLanguageBackground.label;"
                                              accesskey="&filePrefUseSubLanguageBackground.accesskey;"/>
                                    <hbox align="center" style="margin-left: 27px;">
                                        <label value="&filePrefSubLanguageBackground.label;"
                                               id="specificColorPickerSubLanguageBack_label"
                                               accesskey="&filePrefSubLanguageBackground.accesskey;"/>
                                        <colorpicker id="specificColorPickerSubLanguageBack"
                                                     palettename="scin-customize"
                                                     type="button" width="20" height="20"
                                                     persist="palettename"
                                                     onchange="setColour(this);" />
                                        <button id="specificColorPickerSubLanguageBack_customColorPicker"
                                                style="margin-bottom: 6px"
                                                class="arrow-right-6x3-icon button-toolbar-a"
                                                oncommand="customColor('specificColorPickerSubLanguageBack')"/>
                                    </hbox>
                                </vbox>
                            </vbox>
                        </hbox>
                    </tabpanel>


                    <tabpanel id="indicators_panel" orient="vertical">
                        <description style="margin-bottom: 10px;">
                            &indicators.description;
                        </description>
                        <vbox>
                            <hbox align="center">
                                <label value="&indicatorMenu.label;" accesskey="&filePrefAllLangElementType1.accesskey;" control="indicator_menulist"/>
                                <menulist id="indicator_menulist"
                                          oncommand="updateIndicatorStyle();">
                                    <menupopup id="indicator_popup"/>
                                </menulist>
                            </hbox>

                            <separator class="thin"/>

                            <hbox align="center" style="margin-left: 27px;">
                                <label value="&indicStyle.label;"/>
                                <menulist id="indicator_style_menulist"
                                          oncommand="setIndicator();">
                                    <menupopup id="indicator_style_popup">
                                        <menuitem id="indicator_style_plain_menuitem_0"
                                                  value="0"
                                                  label="&indicStylePlain.label;" />
                                        <menuitem id="indicator_style_menuitem_1"
                                                  value="1"
                                                  label="&indicStyleSquiggle.label;" />
                                        <menuitem id="indicator_style_menuitem_2"
                                                  value="2"
                                                  label="&indicStyleTT.label;" />
                                        <menuitem id="indicator_style_menuitem_3"
                                                  value="3"
                                                  label="&indicStyleDiagonal.label;" />
                                        <menuitem id="indicator_style_menuitem_4"
                                                  value="4"
                                                  label="&indicStyleStrike.label;" />
                                        <menuitem id="indicator_style_menuitem_5"
                                                  value="5"
                                                  label="&indicStyleHidden.label;" />
                                        <menuitem id="indicator_style_menuitem_6"
                                                  value="6"
                                                  label="&indicStyleBox.label;" />
                                        <menuitem id="indicator_style_menuitem_7"
                                                  value="7"
                                                  label="&indicStyleRoundBox.label;" />
                                    </menupopup>
                                </menulist>
                            </hbox>

                            <separator class="thin"/>

                            <hbox align="center" style="margin-left: 27px;">
                                <label value="&indicColor.label;"/>
                                <colorpicker id="indicator_color"
                                             palettename="scin-customize"
                                             type="button" width="20" height="20"
                                             persist="palettename"
                                             onchange="setIndicator()"/>
                                <button style="margin-bottom: 6px"
                                        class="arrow-right-6x3-icon button-toolbar-a"
                                        oncommand="customColor('indicator_color')"/>

                                <spacer class="thick" style="margin-left: 27px;"/>

                                <label value="&indicAlpha.label;"/>
                                <textbox id="indicator_alpha_textbox"
                                         size="3"
                                         oninput="setIndicator()"/>
                            </hbox>

                            <separator class="thin"/>

                            <hbox align="center" style="margin-left: 27px;">
                                <checkbox id="indicator_draw_underneath_checkbox"
                                          label="&indicDrawUnderneath.label;"
                                          oncommand="setIndicator()" />
                            </hbox>
                        </vbox>
                    </tabpanel>
                </tabpanels>
            </tabbox>
        </vbox>

        <separator class="thin"/>

      <!-- Custom color picker to use -->
        <vbox id="colorpicker_vbox" style="padding: 6px;">
            <hbox id="colorpicker_hbox" align="center">
                <label id="colorpicker_label"
                       value="Color Picker to use:"/>
                <menulist id="colorpicker_menulist"
                          oncommand="setColorpicker();">
                    <menupopup id="colorpicker_menupopup" />
                </menulist>
                <button id="colorpicker_test_button"
                        oncommand="testColorpicker();"
                        label="Test"/>
            </hbox>
        </vbox>

    </vbox>

</window>


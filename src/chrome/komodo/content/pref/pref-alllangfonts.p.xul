                                        <button style="margin-bottom: 6px"
                                                class="arrow-right-6x3-icon button-toolbar-a"
                                                oncommand="customColor('fixedColorPickerBack')"/>
                                    </hbox>
                                </vbox>
                            </hbox>
                        </vbox>
                        <vbox flex="1">
                            <hbox align="center">
                                <label value="&filePrefAllLangProportional.label;"

                                          id="fixed"
                                          oncommand="onClickFixedFont()">
                                    <menupopup/>
                                </menulist>
                            </hbox>
                            <separator class="thin"/>
                            <hbox align="center">
                                <menulist id="fixedSize" oncommand="onClickFixedSize()">
                                    <menupopup>
                                        <menuitem id="fixedSize_8" value="8" label="8"/>
                                        <menuitem id="fixedSize_9" value="9" label="9"/>
                                        <menuitem id="fixedSize_10" value="10" label="10"/>
                                        <menuitem id="fixedSize_11" value="11" label="11"/>
                                        <menuitem id="fixedSize_12" value="12" label="12"/>
                                        <menuitem id="fixedSize_13" value="13" label="13"/>
                                        <menuitem id="fixedSize_14" value="14" label="14"/>
                                        <menuitem id="fixedSize_15" value="15" label="15"/>
                                        <menuitem id="fixedSize_16" value="16" label="16"/>
                                        <menuitem id="fixedSize_18" value="18" label="18"/>
                                        <menuitem id="fixedSize_20" value="20" label="20"/>
                                        <menuitem id="fixedSize_24" value="24" label="24"/>
                                        <menuitem id="fixedSize_26" value="26" label="26"/>
                                        <menuitem id="fixedSize_28" value="28" label="28"/>
                                        <menuitem id="fixedSize_32" value="32" label="32"/>
                                        <menuitem id="fixedSize_36" value="36" label="36"/>
                                        <menuitem id="fixedSize_40" value="40" label="40"/>
                                        <menuitem id="fixedSize_48" value="48" label="48"/>
                                        <menuitem id="fixedSize_56" value="56" label="56"/>
                                        <menuitem id="fixedSize_64" value="64" label="64"/>
                                        <menuitem id="fixedSize_72" value="72" label="72"/>
                                    </menupopup>
                                </menulist>
                                <button id="fixedBold"
                                        type="checkbox"
                                        class="boldtext-icon button-toolbar-d"
                                        oncommand="onClickBoldOrItalic(this)" />
                                <button id="fixedItalic"
                                        type="checkbox"
                                        class="italictext-icon button-toolbar-d"
                                        oncommand="onClickBoldOrItalic(this)" />
                                <vbox>
                                    <hbox align="center">

                                        <menuitem id="propSize_72" value="72" label="72"/>
                                    </menupopup>
                                </menulist>
                                <button id="propBold"
                                        class="boldtext-icon button-toolbar-d"
                                        type="checkbox"
                                        oncommand="onClickBoldOrItalic(this)"/>
                                <button id="propItalic"
                                        type="checkbox"
                                        class="italictext-icon button-toolbar-d"
                                        oncommand="onClickBoldOrItalic(this)"/>
                                <vbox>
                                    <hbox align="center">
                                        <label value="&filPrefAllLangForeText;"/>
                                        <colorpicker id="propColorPickerFore"

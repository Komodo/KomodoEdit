                                        <menuitem id="propSize_20" value="20" label="20"/>
                                        <menuitem id="propSize_24" value="24" label="24"/>
                                        <menuitem id="propSize_26" value="26" label="26"/>
                                        <menuitem id="propSize_28" value="28" label="28"/>
                                        <menuitem id="propSize_32" value="32" label="32"/>
                                        <menuitem id="propSize_36" value="36" label="36"/>
                                        <menuitem id="propSize_40" value="40" label="40"/>
